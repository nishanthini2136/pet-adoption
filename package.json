{
  "name": "petadoptionplatform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run install-client && npm run build-client",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.1.10"
  },
  "proxy": "http://localhost:5000"
}
