<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f0f8ff"/>
  <!-- Bird body -->
  <ellipse cx="200" cy="180" rx="45" ry="30" fill="#f4a460"/>
  <!-- Bird head -->
  <circle cx="200" cy="140" r="30" fill="#f4a460"/>
  <!-- Beak -->
  <polygon points="200,140 215,135 215,145" fill="#ffd700"/>
  <!-- Eyes -->
  <circle cx="190" cy="130" r="3" fill="#000"/>
  <circle cx="210" cy="130" r="3" fill="#000"/>
  <!-- Wings -->
  <ellipse cx="160" cy="170" rx="25" ry="18" fill="#daa520"/>
  <ellipse cx="240" cy="170" rx="25" ry="18" fill="#daa520"/>
  <!-- Tail -->
  <polygon points="245,180 270,170 270,190" fill="#daa520"/>
  <!-- Feet -->
  <line x1="190" y1="210" x2="190" y2="225" stroke="#ffd700" stroke-width="2"/>
  <line x1="210" y1="210" x2="210" y2="225" stroke="#ffd700" stroke-width="2"/>
  <!-- Crest -->
  <polygon points="200,110 190,90 210,90" fill="#daa520"/>
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#daa520">Cockatiel</text>
</svg>
