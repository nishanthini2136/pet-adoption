<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f8f4e6"/>
  <!-- Dog body -->
  <ellipse cx="200" cy="180" rx="80" ry="40" fill="#d4a574"/>
  <!-- Dog head -->
  <circle cx="200" cy="120" r="50" fill="#d4a574"/>
  <!-- Ears -->
  <ellipse cx="170" cy="90" rx="15" ry="25" fill="#b8860b"/>
  <ellipse cx="230" cy="90" rx="15" ry="25" fill="#b8860b"/>
  <!-- Eyes -->
  <circle cx="185" cy="110" r="5" fill="#000"/>
  <circle cx="215" cy="110" r="5" fill="#000"/>
  <!-- Nose -->
  <ellipse cx="200" cy="130" rx="8" ry="5" fill="#000"/>
  <!-- Mouth -->
  <path d="M 190 140 Q 200 150 210 140" stroke="#000" stroke-width="2" fill="none"/>
  <!-- Tail -->
  <path d="M 280 180 Q 320 160 330 190" stroke="#d4a574" stroke-width="8" fill="none"/>
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#8b4513">Golden Retriever</text>
</svg>
