<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#fff8dc"/>
  <!-- Rabbit body -->
  <ellipse cx="200" cy="180" rx="60" ry="35" fill="#f5deb3"/>
  <!-- Rabbit head -->
  <circle cx="200" cy="130" r="40" fill="#f5deb3"/>
  <!-- Long ears -->
  <ellipse cx="180" cy="70" rx="8" ry="35" fill="#deb887"/>
  <ellipse cx="220" cy="70" rx="8" ry="35" fill="#deb887"/>
  <!-- Eyes -->
  <circle cx="185" cy="115" r="4" fill="#000"/>
  <circle cx="215" cy="115" r="4" fill="#000"/>
  <!-- Nose -->
  <ellipse cx="200" cy="125" rx="3" ry="2" fill="#ff69b4"/>
  <!-- Mouth -->
  <path d="M 195 130 Q 200 135 205 130" stroke="#000" stroke-width="1" fill="none"/>
  <!-- Whiskers -->
  <line x1="190" y1="125" x2="170" y2="120" stroke="#000" stroke-width="1"/>
  <line x1="190" y1="130" x2="170" y2="130" stroke="#000" stroke-width="1"/>
  <line x1="210" y1="125" x2="230" y2="120" stroke="#000" stroke-width="1"/>
  <line x1="210" y1="130" x2="230" y2="130" stroke="#000" stroke-width="1"/>
  <!-- Tail -->
  <circle cx="260" cy="180" r="8" fill="#f5deb3"/>
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#8b4513">Holland Lop</text>
</svg>
