<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#fff8dc"/>
  <!-- Rabbit body -->
  <ellipse cx="200" cy="180" rx="50" ry="30" fill="#f5deb3"/>
  <!-- Rabbit head -->
  <circle cx="200" cy="140" r="35" fill="#f5deb3"/>
  <!-- Short ears -->
  <ellipse cx="185" cy="85" rx="6" ry="20" fill="#deb887"/>
  <ellipse cx="215" cy="85" rx="6" ry="20" fill="#deb887"/>
  <!-- Eyes -->
  <circle cx="190" cy="120" r="3" fill="#000"/>
  <circle cx="210" cy="120" r="3" fill="#000"/>
  <!-- Nose -->
  <ellipse cx="200" cy="130" rx="2" ry="1" fill="#ff69b4"/>
  <!-- Mouth -->
  <path d="M 195 135 Q 200 138 205 135" stroke="#000" stroke-width="1" fill="none"/>
  <!-- Whiskers -->
  <line x1="190" y1="125" x2="175" y2="120" stroke="#000" stroke-width="1"/>
  <line x1="190" y1="130" x2="175" y2="130" stroke="#000" stroke-width="1"/>
  <line x1="210" y1="125" x2="225" y2="120" stroke="#000" stroke-width="1"/>
  <line x1="210" y1="130" x2="225" y2="130" stroke="#000" stroke-width="1"/>
  <!-- Tail -->
  <circle cx="250" cy="180" r="6" fill="#f5deb3"/>
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#8b4513">Netherland Dwarf</text>
</svg>
